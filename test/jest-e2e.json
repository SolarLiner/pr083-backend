{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@pr083/level/(.*)": "<rootDir>/../libs/level/src/$1",
    "@pr083/level": "<rootDir>/../libs/level/src",
    "@pr083/rest-utils/(.*)": "<rootDir>/../libs/rest-utils/src/$1",
    "@pr083/rest-utils": "<rootDir>/../libs/rest-utils/src",
    "@pr083/user/(.*)": "<rootDir>/../libs/user/src/$1",
    "@pr083/user": "<rootDir>/../libs/user/src",
    "@pr083/mail/(.*)": "<rootDir>/../libs/mail/src/$1",
    "@pr083/mail": "<rootDir>/../libs/mail/src",
    "@pr083/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@pr083/auth": "<rootDir>/../libs/auth/src",
    "@pr083/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@pr083/config": "<rootDir>/../libs/config/src"
  }
}